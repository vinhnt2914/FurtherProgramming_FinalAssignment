<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.UpdateClaimController">
    <Label text="Edit Claim Details" style="-fx-font-size: 18px;"/>
    <Label text="Claim Amount:"/>
    <TextField fx:id="claimAmountTextField"/>
    <Label text="Banking Info:"/>
    <TextField fx:id="bankingInfoTextField"/>
    <Button text="Update" onAction="#updateClaim"/>
</VBox>
