<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="customerDashBoard" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="400.0" minWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.app.controllers.CustomerDashboard">
   <center>
      <HBox fillHeight="false" BorderPane.alignment="CENTER">
         <children>
            <VBox spacing="30.0">
               <children>
                  <Label fx:id="fullNameLabel" maxWidth="350.0" minWidth="300.0" text="Full Name: " wrapText="true">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label fx:id="addressLabel" maxWidth="350.0" minWidth="300.0" text="Address: " wrapText="true">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <VBox spacing="30.0">
               <children>
                  <Label fx:id="phoneLabel" maxWidth="350.0" minWidth="300.0" text="Phone: " wrapText="true">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label fx:id="emailLabel" maxWidth="350.0" minWidth="300.0" text="Email: " wrapText="true">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </children>
      </HBox>
   </center>
   <padding>
      <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
   </padding>
</BorderPane>
